#include<bits/stdc++.h>      // T.C : O(log n)
using namespace std;                         
int binarySearch(int arr[], int first, int last, int x) 
{ 
    while(first<=last)
    {
       int mid=first+(last-first)/2;
       
        if (arr[mid] == x) 
            return arr[mid]; 
            
      if (arr[mid] < x) 
            first = mid + 1; 
       
        else
            last = mid - 1; 
    } 
    
    int a = abs(arr[first] - x);
    int b = abs(arr[last] - x);
    if(a>b)  return arr[last];
    else
    return arr[first];
} 
int main()
{
   int n;
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++)  cin>>arr[i];
   int x;
   cin>>x;
  
	int res = binarySearch(arr,0,n-1, x) ; 
    cout << res; 
}   
